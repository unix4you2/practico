DROP TABLE IF EXISTS core_bugtracker;
CREATE TABLE core_bugtracker (
  id int(10) NOT NULL AUTO_INCREMENT,
  fecha_apertura datetime,
  fecha_actualizacion datetime,
  fecha_cierre datetime,
  proyecto varchar(255),
  proyecto_version varchar(30),
  titulo varchar(255),
  descripcion text,
  categoria varchar(255),
  estado varchar(20),
  severidad varchar(20),
  prioridad varchar(20),
  reportado_por varchar(250),
  asignado_a varchar(250),
  pasos_reproduccion text,
  sistema_origen varchar(255),
  trazas text,
  adjunto longblob,
  historial_gestion text,
  PRIMARY KEY (id)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci;

DROP TABLE IF EXISTS core_tabla_comodin;
CREATE TABLE core_tabla_comodin (
  id int(10) NOT NULL AUTO_INCREMENT,
  PRIMARY KEY (id)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci;

DROP TABLE IF EXISTS core_tareascron;
CREATE TABLE core_tareascron (
  id int(10) NOT NULL AUTO_INCREMENT,
  titulo varchar(255),
  fecha_creacion date,
  codigo_tarea varchar(255),
  script_php text,
  habilitado int(1),
  historial_ejecucion text,
  PRIMARY KEY (id)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci;

DROP TABLE IF EXISTS core_pcoder_historial;
CREATE TABLE core_pcoder_historial (
  id int(10) NOT NULL AUTO_INCREMENT,
  archivo text,
  fecha_edicion datetime,
  usuario varchar(255),
  lineas int(11),
  caracteres int(11),
  contenido mediumtext,
  tipo_origen char(1) NULL DEFAULT 'A',
  PRIMARY KEY (id)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci;

DROP TABLE IF EXISTS core_pcoder_bloqueos;
CREATE TABLE core_pcoder_bloqueos (
  id int(10) NOT NULL AUTO_INCREMENT,
  archivo text,
  ultima_edicion datetime,
  usuario varchar(255),
  abierto int(1),
  direccion_origen varchar(255),
  agente varchar(255),
  PRIMARY KEY (id)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci;

DROP TABLE IF EXISTS core_pcoder_chat;
CREATE TABLE core_pcoder_chat (
  id int(11) UNSIGNED NOT NULL AUTO_INCREMENT,
  remitente VARCHAR(255) NOT NULL DEFAULT '',
  destinatario VARCHAR(255) NOT NULL DEFAULT '',
  message TEXT NOT NULL,
  sent DATETIME,
  recd int(11) UNSIGNED NOT NULL DEFAULT 0,
  PRIMARY KEY (id)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci;

DROP TABLE IF EXISTS core_acortadorurls;
CREATE TABLE core_acortadorurls (
  id int(11) unsigned NOT NULL AUTO_INCREMENT,
  url_larga varchar(1024) NOT NULL,
  url_corta varchar(10) NOT NULL,
  usuario varchar(255) NOT NULL,
  fecha_creacion date NOT NULL,
  hora_creacion time NOT NULL,
  contador_uso int(11) NOT NULL DEFAULT '0',
  tipo_redireccion char(3) NOT NULL DEFAULT '301',
  PRIMARY KEY (id)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci;

DROP TABLE IF EXISTS core_samlconector;
CREATE TABLE core_samlconector (
  id int(11) unsigned NOT NULL AUTO_INCREMENT,
  activado char(1) DEFAULT 'S',
  nombre_conector varchar(255) NOT NULL,
  ruta_logo varchar(255) NOT NULL,
  sp_baseurl varchar(255) NOT NULL,
  sp_entity_id varchar(255) NOT NULL,
  sp_assertion_consumer_service varchar(255) NOT NULL,
  sp_single_logout_service varchar(255) NOT NULL,
  sp_nameid_format varchar(255) NOT NULL,
  sp_nameid_format_full varchar(255) NOT NULL,
  idp_entityid varchar(255) NOT NULL,
  idp_single_signon_service varchar(255) NOT NULL,
  idp_single_logout_service varchar(255) NOT NULL,
  idp_x509cert text,
  usuario_plantilla varchar(255) NOT NULL,
  PRIMARY KEY (id)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci;

DROP TABLE IF EXISTS core_modulos_app;
CREATE TABLE core_modulos_app (
  id int(11) NOT NULL AUTO_INCREMENT,
  nombre varchar(250) NOT NULL,
  descripcion text,
  desarrolladores text,
  PRIMARY KEY  (id),
  UNIQUE (id)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci;

DROP TABLE IF EXISTS core_scripts;
CREATE TABLE core_scripts (
  id int(10) AUTO_INCREMENT,
  titulo varchar(255) default '',
  codigo_script varchar(255),
  fecha_creacion date NOT NULL,
  descripcion varchar(255) default '',
  creado_por varchar(255) default '',
  lenguaje varchar(20) default '',
  cuerpo mediumtext,
  es_snippet int(1) DEFAULT '0',
  moduloapp int(11),
  ejecuciones int(11) DEFAULT '0',
  modo_ejecucion varchar(20) default 'system',
  PRIMARY KEY  (id)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci;
ALTER TABLE core_scripts ADD INDEX codigo_script (codigo_script);
