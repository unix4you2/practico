DROP TABLE IF EXISTS core_parametros;
CREATE TABLE core_parametros (
  id int(10) NOT NULL AUTO_INCREMENT,
  nombre_empresa_largo varchar(250) NOT NULL,
  nombre_empresa_corto varchar(50) NOT NULL,
  nombre_aplicacion varchar(100) NOT NULL,
  version varchar(20) NULL DEFAULT '1.0',
  fecha_lanzamiento date,
  licencia text,
  creditos text,
  funciones_personalizadas text,
  federado_servidor varchar(255) NULL DEFAULT '',
  federado_usuario varchar(255) NULL DEFAULT '',
  federado_clave varchar(255) NULL DEFAULT '',
  federado_motor varchar(255) NULL DEFAULT '',
  federado_basedatos varchar(255) NULL DEFAULT '',
  federado_tabla varchar(255) NULL DEFAULT '',
  federado_campousuario varchar(255) NULL DEFAULT '',
  federado_campoclave varchar(255) NULL DEFAULT '',
  federado_encripcion varchar(255) NULL DEFAULT '',
  federado_puerto varchar(255) NULL DEFAULT '',
  requiere_aceptar_licencia int(1) NULL DEFAULT '0',
  api_openai varchar(255) NULL DEFAULT '',
  url_openai varchar(255) NULL DEFAULT 'https://api.openai.com/v1',
  usuarios_desarroladores varchar(1024) NULL DEFAULT 'admin',
  usuarios_admin_kanban varchar(1024) NULL DEFAULT 'admin',
  api_googleai varchar(255) NULL DEFAULT '',
  url_googleai varchar(255) NULL DEFAULT 'https://',
  facialrec_habilitar char(1) DEFAULT 'N',
  facialrec_nromuestras int(10) DEFAULT '1',
  facialrec_anchocam int(10) DEFAULT '320',
  facialrec_altocam int(10) DEFAULT '240',
  facialrec_tiempomuestreo int(10) DEFAULT '3',
  facialrec_precision DECIMAL(2,1) DEFAULT '0.5',
  PRIMARY KEY  (id)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci;

