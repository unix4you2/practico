INSERT INTO core_scripts_lenguajes (id, nombre, extensiones, comando_compilacion, comando_ejecucion, hola_mundo) VALUES (6,	'Assembly_x86',	'asm',	'',	'',	'DOSSEG\r\n.MODEL TINY\r\n.DATA\r\nTXT DB \"Practico Framework! Prueba de lenguaje ASSEMBLY_x86 generada por el usuario { $ PCOSESS_LoginUsuario} { $ PCO_FechaOperacionGuiones} { $ PCO_HoraOperacionPuntos}$\"\r\n.CODE\r\nSTART:\r\n	MOV ax, @DATA\r\n	MOV ds, ax\r\n	\r\n	MOV ah, 09h		; prepare output function\r\n	MOV dx, OFFSET TXT	; set offset\r\n	INT 21h			; output string TXT\r\n	\r\n	MOV AX, 4C00h 		; go back to DOS\r\n	INT 21h\r\nEND START');
