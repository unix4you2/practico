DROP TABLE IF EXISTS core_documentacion;
CREATE TABLE core_documentacion (
  id int(11) NOT NULL AUTO_INCREMENT,
  hash_unico varchar(255) NOT NULL,
  titulo varchar(255) NOT NULL,
  estructura_tipo varchar(10) NOT NULL COMMENT 'Seccion,Menu,Funcion',
  estructura_seccion varchar(255) NOT NULL,
  estructura_funcion varchar(255) NOT NULL,
  origen varchar(10) NOT NULL COMMENT 'Archivo,Formulario,Informe',
  detalle_origen varchar(255) NOT NULL COMMENT 'IdObjeto,Path',
  formato varchar(255) NOT NULL DEFAULT 'Enriquecido' COMMENT 'Enriquecido,Diagrama',
  documentacion mediumtext NOT NULL,
  visibilidad varchar(255) NOT NULL DEFAULT 'Desarrollo' COMMENT 'Desarrollo,Usuario',
  PRIMARY KEY (id)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;


DROP TABLE IF EXISTS core_usuario;
CREATE TABLE core_usuario (
  id int(11) NOT NULL AUTO_INCREMENT,
  login varchar(250) NOT NULL,
  clave varchar(50) NOT NULL default 'd41d8cd98fd41d8cd98fd41d8cd98fd41d8cd98f',
  nombre varchar(250) NOT NULL default '',
  estado int(1) NOT NULL default '1',
  correo varchar(200) NOT NULL default '',
  ultimo_acceso date NOT NULL default '20000101',
  llave_paso varchar(50) NOT NULL default 'd41d8cd98f00b204e9800998ecf8427e',
  usuario_interno int(1) NULL DEFAULT 0,
  llave_recuperacion varchar(250) NOT NULL default '',
  es_plantilla int(1) DEFAULT 0,
  plantilla_permisos varchar(250) default '',
  descripcion varchar(250) NULL,
  transformacion_colores varchar(10) NULL,
  idioma varchar(5) NOT NULL DEFAULT '',
  avatar varchar(255) NOT NULL DEFAULT '',
  redes_permitidas text,
  vectores_faciales text,
  PRIMARY KEY  (login),
  UNIQUE (id)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci;

